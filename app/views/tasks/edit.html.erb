<div class="container col-6 text-center">
<h1>Edit <%= @task.title %></h1>
<%= form_for @task do |f|%>
<div class="form-group">
<%= f.label :title %>
<%= f.text_field :title %>
</div>
<div class="form-group">
<%= f.label :details %>
<%= f.text_field :details %>
<div>
<%= f.submit class: "btn btn-primary " %>
</div>
</div>
<% end %>
</div>